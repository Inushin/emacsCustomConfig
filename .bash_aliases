alias e="emacs -nw"
alias rgrep="grep -r --color --exclude='*~'"
alias gl='git log --graph --pretty=format:"%C(yellow)%h%Creset %Cgreen(%cd) %C(bold blue)<%an>%Creset %C(red)%d%Creset %s" --all'
alias gs="git status"
alias gco="git commit -m"
alias gd="git --no-pager diff"
alias ga="git add"
alias sysup="sudo apt update && sudo apt upgrade -y && sudo apt autoremove -y""
alias lsa="ls -lah"
